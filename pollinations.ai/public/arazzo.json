{
  "schema_version": "1.0.0",
  "metadata": {
    "title": "Pollinations.AI",
    "description": "Free and open-source API for text, image, and audio generation with no signups or API keys required",
    "version": "1.0.0",
    "contact": {
      "name": "Pollinations.AI Team",
      "url": "https://discord.gg/k9F7SyTgqn"
    },
    "license": {
      "name": "MIT",
      "url": "https://github.com/pollinations/pollinations/blob/master/LICENSE"
    },
    "last_updated": "2025-04-19"
  },
  "services": [
    {
      "name": "Image Generation",
      "description": "Generate images from text prompts",
      "endpoint": "https://image.pollinations.ai/prompt/{prompt}",
      "method": "GET",
      "parameters": [
        {
          "name": "prompt",
          "description": "Text description of the image to generate",
          "required": true,
          "type": "string",
          "in": "path"
        },
        {
          "name": "model",
          "description": "Model to use for generation",
          "required": false,
          "type": "string",
          "default": "flux",
          "in": "query"
        },
        {
          "name": "seed",
          "description": "Seed for reproducible results",
          "required": false,
          "type": "integer",
          "in": "query"
        },
        {
          "name": "width",
          "description": "Width of the generated image",
          "required": false,
          "type": "integer",
          "default": 1024,
          "in": "query"
        },
        {
          "name": "height",
          "description": "Height of the generated image",
          "required": false,
          "type": "integer",
          "default": 1024,
          "in": "query"
        },
        {
          "name": "nologo",
          "description": "Turn off the rendering of the logo",
          "required": false,
          "type": "boolean",
          "default": false,
          "in": "query"
        },
        {
          "name": "enhance",
          "description": "Turn on prompt enhancing",
          "required": false,
          "type": "boolean",
          "default": false,
          "in": "query"
        },
        {
          "name": "safe",
          "description": "Enable strict NSFW content filtering",
          "required": false,
          "type": "boolean",
          "default": false,
          "in": "query"
        }
      ],
      "response": {
        "type": "image",
        "format": "jpeg/png"
      },
      "examples": [
        {
          "description": "Generate an image of a sunset",
          "request": "https://image.pollinations.ai/prompt/A%20beautiful%20sunset%20over%20the%20ocean?width=1280&height=720&seed=42"
        }
      ]
    },
    {
      "name": "Text Generation",
      "description": "Generate text from prompts",
      "endpoint": "https://text.pollinations.ai/{prompt}",
      "method": "GET",
      "parameters": [
        {
          "name": "prompt",
          "description": "Text prompt for the AI to respond to",
          "required": true,
          "type": "string",
          "in": "path"
        },
        {
          "name": "model",
          "description": "Model to use for text generation",
          "required": false,
          "type": "string",
          "default": "openai",
          "in": "query"
        },
        {
          "name": "seed",
          "description": "Seed for reproducible results",
          "required": false,
          "type": "integer",
          "in": "query"
        },
        {
          "name": "json",
          "description": "Receive response in JSON format",
          "required": false,
          "type": "boolean",
          "default": false,
          "in": "query"
        },
        {
          "name": "system",
          "description": "System prompt to set the behavior of the AI",
          "required": false,
          "type": "string",
          "in": "query"
        }
      ],
      "response": {
        "type": "text",
        "format": "plain/json"
      },
      "examples": [
        {
          "description": "Generate text about AI",
          "request": "https://text.pollinations.ai/What%20is%20artificial%20intelligence?model=mistral&seed=42"
        }
      ]
    },
    {
      "name": "Text Generation (POST)",
      "description": "Generate text using a POST request with messages",
      "endpoint": "https://text.pollinations.ai/",
      "method": "POST",
      "requestBody": {
        "content": "application/json",
        "schema": {
          "type": "object",
          "required": ["messages"],
          "properties": {
            "messages": {
              "type": "array",
              "items": {
                "type": "object",
                "required": ["role", "content"],
                "properties": {
                  "role": {
                    "type": "string",
                    "enum": ["system", "user", "assistant"]
                  },
                  "content": {
                    "type": "string"
                  }
                }
              }
            },
            "model": {
              "type": "string",
              "default": "openai"
            },
            "seed": {
              "type": "integer"
            },
            "jsonMode": {
              "type": "boolean",
              "default": false
            },
            "tools": {
              "type": "array",
              "description": "Tools that the model can use",
              "items": {
                "type": "object"
              }
            }
          }
        }
      },
      "response": {
        "type": "json",
        "schema": {
          "type": "object",
          "properties": {
            "response": {
              "type": "string"
            },
            "model": {
              "type": "string"
            },
            "tool_calls": {
              "type": "array",
              "items": {
                "type": "object"
              }
            }
          }
        }
      },
      "examples": [
        {
          "description": "Generate text with a system prompt",
          "request": {
            "method": "POST",
            "url": "https://text.pollinations.ai/",
            "headers": {
              "Content-Type": "application/json"
            },
            "body": {
              "messages": [
                {"role": "system", "content": "You are a helpful assistant."},
                {"role": "user", "content": "What is artificial intelligence?"}
              ],
              "model": "mistral",
              "seed": 42
            }
          }
        },
        {
          "description": "Generate text with function calling",
          "request": {
            "method": "POST",
            "url": "https://text.pollinations.ai/",
            "headers": {
              "Content-Type": "application/json"
            },
            "body": {
              "messages": [
                {"role": "user", "content": "What's the weather in New York?"}
              ],
              "model": "openai",
              "tools": [
                {
                  "type": "function",
                  "function": {
                    "name": "get_weather",
                    "description": "Get the current weather in a given location",
                    "parameters": {
                      "type": "object",
                      "properties": {
                        "location": {
                          "type": "string",
                          "description": "The city and state"
                        },
                        "unit": {
                          "type": "string",
                          "enum": ["celsius", "fahrenheit"]
                        }
                      },
                      "required": ["location"]
                    }
                  }
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "OpenAI-Compatible Chat Completions",
      "description": "Standard OpenAI-compatible endpoint for chat completions",
      "endpoint": "https://text.pollinations.ai/v1/chat/completions",
      "method": "POST",
      "requestBody": {
        "content": "application/json",
        "schema": {
          "type": "object",
          "properties": {
            "messages": {
              "type": "array",
              "items": {
                "type": "object"
              }
            },
            "model": {
              "type": "string"
            },
            "stream": {
              "type": "boolean"
            },
            "temperature": {
              "type": "number"
            },
            "max_tokens": {
              "type": "integer"
            },
            "tools": {
              "type": "array",
              "items": {
                "type": "object"
              }
            }
          }
        }
      },
      "response": {
        "type": "json",
        "format": "application/json"
      },
      "examples": [
        {
          "description": "Standard OpenAI-compatible chat completion request",
          "request": {
            "method": "POST",
            "url": "https://text.pollinations.ai/v1/chat/completions",
            "headers": {
              "Content-Type": "application/json"
            },
            "body": {
              "messages": [
                {"role": "system", "content": "You are a helpful assistant."},
                {"role": "user", "content": "Hello, who are you?"}
              ],
              "model": "openai"
            }
          }
        }
      ]
    },
    {
      "name": "Audio Generation",
      "description": "Convert text to speech with various voice options",
      "endpoint": "https://text.pollinations.ai/",
      "method": "POST",
      "requestBody": {
        "content": "application/json",
        "schema": {
          "type": "object",
          "required": ["model", "input"],
          "properties": {
            "model": {
              "type": "string",
              "description": "Audio model to use",
              "default": "openai-audio"
            },
            "input": {
              "type": "string",
              "description": "Text to convert to speech"
            },
            "voice": {
              "type": "string",
              "description": "Voice to use for speech synthesis",
              "enum": ["alloy", "echo", "fable", "onyx", "nova", "shimmer"],
              "default": "alloy"
            },
            "response_format": {
              "type": "string",
              "description": "Format of the output audio",
              "enum": ["mp3", "opus", "aac", "flac", "pcm", "wav"],
              "default": "mp3"
            },
            "speed": {
              "type": "number",
              "description": "Speed of the generated audio",
              "minimum": 0.25,
              "maximum": 4.0,
              "default": 1.0
            }
          }
        }
      },
      "response": {
        "type": "audio",
        "format": "mp3/opus/wav"
      },
      "examples": [
        {
          "description": "Convert text to speech",
          "request": {
            "method": "POST",
            "url": "https://text.pollinations.ai/",
            "headers": {
              "Content-Type": "application/json"
            },
            "body": {
              "model": "openai-audio",
              "input": "Hello world! This is a text-to-speech example using Pollinations.AI.",
              "voice": "alloy",
              "response_format": "mp3"
            }
          }
        }
      ]
    }
  ],
  "models": {
    "text": [
      {
        "id": "openai",
        "name": "OpenAI GPT",
        "description": "Default model for text generation"
      },
      {
        "id": "openai-4",
        "name": "GPT-4",
        "description": "OpenAI's advanced reasoning model"
      },
      {
        "id": "mistral",
        "name": "Mistral",
        "description": "Open-source large language model"
      },
      {
        "id": "mistral-small",
        "name": "Mistral Small 24B",
        "description": "Smaller, faster Mistral model"
      },
      {
        "id": "claude",
        "name": "Claude",
        "description": "Anthropic's Claude model"
      },
      {
        "id": "gemini",
        "name": "Gemini",
        "description": "Google's Gemini model with Vertex AI integration"
      },
      {
        "id": "phi-mini",
        "name": "Phi Mini",
        "description": "Microsoft's compact and efficient model"
      },
      {
        "id": "qwen-reasoning",
        "name": "Qwen Reasoning",
        "description": "Advanced reasoning capabilities"
      },
      {
        "id": "o3-mini",
        "name": "O3 Mini",
        "description": "Compact version of OpenAI's o1 model"
      }
    ],
    "image": [
      {
        "id": "flux",
        "name": "FLUX",
        "description": "Default model for image generation"
      },
      {
        "id": "sdxl",
        "name": "Stable Diffusion XL",
        "description": "High-quality image generation model"
      }
    ],
    "audio": [
      {
        "id": "openai-audio",
        "name": "OpenAI Audio",
        "description": "Text-to-speech model with multiple voice options",
        "voices": ["alloy", "echo", "fable", "onyx", "nova", "shimmer"]
      }
    ]
  },
  "capabilities": {
    "function_calling": {
      "description": "Support for function calling to enable models to perform actions",
      "supported_models": ["openai", "openai-4", "claude", "gemini"]
    },
    "streaming": {
      "description": "Support for streaming responses as they are generated",
      "endpoints": ["/v1/chat/completions", "/openai"]
    },
    "multi_modal": {
      "description": "Support for image inputs in text generation",
      "supported_models": ["openai-4", "gemini", "claude"]
    }
  },
  "authentication": {
    "type": "none",
    "description": "No authentication required. All endpoints are freely accessible."
  },
  "usage_guidelines": {
    "rate_limits": "Best effort service, no explicit rate limits. Please be considerate with usage.",
    "content_policies": "Please respect content policies and avoid generating harmful content.",
    "attribution": "Consider adding attribution when using generated content: 'Generated using Pollinations.AI (https://pollinations.ai)'"
  }
}
