  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run build_backend",
    "test": "react-scripts test --env=node",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "build_backend": "mkdir -p dist && esbuild src/backend/pollinate-cli.js --platform=node --target=node14 --bundle --external:fsevents --external:electron > dist/pollinate.js  && esbuild src/backend/social-post-cli.js --platform=node --target=node14 --bundle --external:fsevents --external:electron > dist/social-post.js && esbuild src/backend/pinning-cli.js --platform=node --target=node14 --bundle --external:fsevents --external:electron > dist/pin.js&& esbuild src/backend/discord-post-cli.js --platform=node --target=node14 --bundle --external:fsevents --external:electron > dist/discord-post.js && chmod ugoa+x dist/*.js",
    "install_backend": "cp -v dist/pollinate.js /usr/local/bin/pollinate && cp -v dist/social-post.js /usr/local/bin/social_post.js && cp -v dist/pin.js /usr/local/bin/pin.js && cp -v dist/discord-post.js /usr/local/bin/discord-post.js && cp -v scripts/*.sh /usr/local/bin"
  },
  "bin": {
    "pollinate": "./src/backend/pollinate-cli.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
